						<Description>Other Amounts to be distributed</Description>
						<LineNumber>Schedule B Line 10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Distribution Amount -->
			<xsd:element name="TotalDistributionAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Distribution Amount</Description>
						<LineNumber>Schedule B Line 11</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1041ScheduleJ"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Tax Exempt Income Amount -->
			<xsd:element name="TaxExemptIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax Exempt Income Amount</Description>
						<LineNumber>Schedule B Line 12</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tentative Income Distribution Interest Deduction Amount -->
			<xsd:element name="TentativeIncmDistriIntDedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tentative Income Distribution Interest Deduction Amount</Description>
						<LineNumber>Schedule B Line 13</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tentative Income Distribution Deduction Amount -->
			<xsd:element name="TentativeIncmDistriDedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tentative Income Distribution Deduction Amount</Description>
						<LineNumber>Schedule B Line 14</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Schedule B Line 15 - Entry comes from Form 1041 first page Line 18 'IncomeDistributionDeductionAmt' -->

			<!-- Schedule G - Tax Computation -->

			<!-- Tax Amount -->
			<xsd:element name="TaxAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax Amount</Description>
						<LineNumber>Schedule G Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Lump Summary Distribution Tax Amount -->
			<xsd:element name="LumpSumDistributionTaxAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Lump Summary Distribution Tax Amount</Description>
						<LineNumber>Schedule G Line 1b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS4972"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Alternative Minimum Tax Amount -->
