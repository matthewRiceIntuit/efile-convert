<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema targetNamespace="http://www.irs.gov/efile" xmlns="http://www.irs.gov/efile"
xmlns:xsd="http://www.w3.org/2001/XMLSchema"
elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">

	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Schema - IRS Form 1041 U.S. Income Tax Return for Estates and Trusts</Description>
			<TaxYear>2014</TaxYear>
			<MaturityLevel>Release 9.5 Drop 4</MaturityLevel>
			<ReleaseDate>10-07-2014</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>

	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>

	<!-- ======================================================================= -->
	<!-- ============================ IRS Form 1041 ============================ -->
	<!-- ======================================================================= -->

	<xsd:element name="IRS1041">
		<xsd:annotation>
			<xsd:documentation>IRS Form 1041</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS1041Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>

					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS1041">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>

					<xsd:attribute name="referenceDocumentId" type="IdListType" >
						<xsd:annotation>
							<xsd:documentation>List of document ID's of forms, schedules, supporting info. etc. attached to this form</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>

					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1099R IRS2210 IRS2210F IRS6198 IRS8082 IRS8275 IRS8275R IRS8582 IRS8582CR IRS8697 IRS8824 IRS8865 IRS8866 IRS8873 IRS8886 IRS8938 IRSW2 IRSW2G BinaryAttachment GeneralDependency GeneralDependencyMedium GeneralDependencySmall ElectionToTreatContributionsAsPaidInPriorTaxYearStatement FidrcyWvdDedExpnssFrm706Stmt Elect1141110gFlowThruEntStatement"/>

					<xsd:attribute name="section642iCd">
						<xsd:simpleType>
							<xsd:restriction base="TextType">
								<xsd:enumeration value="SECTION 642(i) TRUST"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="IRS1041Type">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 1041</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- Form 1041 Short Period Reason Indicator -->
			<xsd:element name="ShortPeriodReason1041Ind" minOccurs="0" maxOccurs="11">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Form 1041 Short Period Reason Indicator</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:enumeration value="FORM 1128 ATTACHED AS A PDF"/>
						<xsd:enumeration value="IRS GRANTED CHANGE IN TAX PERIOD"/>
						<xsd:enumeration value="TRUST IS CHANGING TAX PERIOD UNDER IRC REG 1.442.1(c)"/>
						<xsd:enumeration value="REV PROC 76-10"/>
						<xsd:enumeration value="REV PROC 85-58"/>
						<xsd:enumeration value="REV PROC 87-32"/>
						<xsd:enumeration value="REV PROC 2003-38"/>
						<xsd:enumeration value="REV PROC 2006-46"/>
						<xsd:enumeration value="IRS REGULATION 1.502-76"/>
						<xsd:enumeration value="SECTION 806 OF TAX REFORM ACT OF 1986"/>
						<xsd:enumeration value="TAX PERIOD MONTH CHANGING TO 12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:choice minOccurs="0">
				<!-- Decedents Estate Indicator -->
				<xsd:element name="DecedentsEstateInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Decedents Estate Indicator</Description>
							<LineNumber>A</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Simple Trust Indicator -->
				<xsd:element name="SimpleTrustInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Simple Trust Indicator</Description>
							<LineNumber>A</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Complex Trust Indicator -->
				<xsd:element name="ComplexTrustInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Complex Trust Indicator</Description>
							<LineNumber>A</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Qualified Disability Trust Indicator -->
				<xsd:element name="QualifiedDisabilityTrustInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Qualified Disability Trust Indicator</Description>
							<LineNumber>A</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- ESBTS Portion Only Indicator -->
			<xsd:element name="ESBTSPortionOnlyInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>ESBTS Portion Only Indicator</Description>
						<LineNumber>A</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grantor Type Trust Indicator -->
			<xsd:element name="GrantorTypeTrustInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grantor Type Trust Indicator</Description>
						<LineNumber>A</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="GrantorTypeTrustIncomeAndDeductionsStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Line A, Bankruptcy Estate Chap 7 Indicator - No entry for MeF return -->

			<!-- Line A, Bankruptcy Estate Chap 11 Indicator - No entry for MeF return -->

			<!-- Pooled Income Fund Indicator -->
			<xsd:element name="PooledIncomeFundInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Pooled Income Fund Indicator</Description>
						<LineNumber>A</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="PooledIncomeFundStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Schedule K1 Attached Counts -->
			<xsd:element name="ScheduleK1AttachedCnt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Schedule K1 Attached Counts</Description>
						<LineNumber>B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="IntegerNN7Type">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1041ScheduleK1"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Line C, Estate Or Trust EIN - From EIN in Return Header -->

			<!-- Estate Or Trust Create Date -->
			<xsd:element name="EstateOrTrustCreateDt" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Estate Or Trust Create Date</Description>
						<LineNumber>D</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Non-Exempt Section 4797(a)(1) Indicator -->
			<xsd:element name="NonExemptSec4947a1Ind" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Non-Exempt Section 4797(a)(1) Indicator</Description>
						<LineNumber>E</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Non-Private Foundation Indicator -->
			<xsd:element name="NonPrivateFoundationInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Non-Private Foundation Indicator</Description>
						<LineNumber>E</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Non-Exempt Section 4797(a)(2) Indicator -->
			<xsd:element name="NonExemptSec4947a2Ind" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Non-Exempt Section 4797(a)(2) Indicator</Description>
						<LineNumber>E</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Initial Return Indicator -->
			<xsd:element name="InitialReturnInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Initial Return Indicator</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Future Filing Not Required Indicator (Final Return Checkbox) -->
			<xsd:element name="FutureFilingNotRequiredInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Final Return Checkbox</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fifty Two or Fifty Three Week Filer Indicator -->
			<xsd:element name="FiftyTwoFiftyThreeWeekFilerInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fifty Two or Fifty Three Week Filer Indicator</Description>
						<LineNumber>Additional Data</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amended Return Indicator -->
			<xsd:element name="AmendedReturnInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amended Return Indicator</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="AmendedReturnChanges"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Superseded Return Indicator -->
			<xsd:element name="SupersededReturnInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicates the Return is a Superseded Return</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="AmendedReturnChanges"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Net Operating Loss Carryback Indicator -->
			<xsd:element name="NetOperatingLossCarrybackInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net Operating Loss Carryback Checkbox</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Trust Name Change Indicator -->
			<xsd:element name="TrustNameChangeInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Trust Name Change Indicator</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fiduciary Change Indicator -->
			<xsd:element name="FiduciaryChangeInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fiduciary Change Indicator</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fiduciary Name Change Indicator -->
			<xsd:element name="FiduciaryNameChangeInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fiduciary Name Change Indicator</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fiduciary Address Change Indicator -->
			<xsd:element name="FiduciaryAddressChangeInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fiduciary Address Change Indicator</Description>
						<LineNumber>F</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Section 645 Election Indicator -->
			<xsd:element name="Section645ElectionInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section 645 Election Indicator</Description>
						<LineNumber>G</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="Section645ElectionTrustStatement"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Section 645 Election Trust EIN -->
			<xsd:element name="TrustEIN" type="EINType" minOccurs="0" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section 645 Election Trust EIN</Description>
						<LineNumber>G</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Interest Amount -->
			<xsd:element name="TaxableInterestAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable Interest Amount</Description>
						<LineNumber>1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Ordinary Dividends Amount -->
			<xsd:element name="OrdinaryDividendsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Ordinary Dividends Amount</Description>
						<LineNumber>2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Qualifying Dividend Beneficiaries Amount -->
			<xsd:element name="QlfyDividendBeneficiariesAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Qualifying Dividend Beneficiaries Amount</Description>
						<LineNumber>2b1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Qualifying Dividends Estate Trust Amount -->
			<xsd:element name="QlfyDividendsEstateTrustAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Qualifying Dividends Estate Trust Amount</Description>
						<LineNumber>2b2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Net Profit Or Loss Amount -->
			<xsd:element name="NetProfitOrLossAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net Profit Or Loss Amount</Description>
						<LineNumber>3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1040ScheduleC IRS1040ScheduleCEZ"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Capital Gain Loss Amount -->
			<xsd:element name="CapitalGainLossAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Capital Gain Loss Amount</Description>
						<LineNumber>4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1041ScheduleD"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Net Rental Income Or Loss Amount -->
			<xsd:element name="NetRentalIncomeOrLossAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net Rental Income Or Loss Amount</Description>
						<LineNumber>5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1040ScheduleE"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Net Farm Profit Or Loss Amount -->
			<xsd:element name="NetFarmProfitLossAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net Farm Profit Or Loss Amount</Description>
						<LineNumber>6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1040ScheduleF"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Total Ordinary Gain Loss Amount -->
			<xsd:element name="TotalOrdinaryGainLossAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Ordinary Gain Loss Amount</Description>
						<LineNumber>7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS4797"/>
							<xsd:attribute name="form4684Cd">
								<xsd:simpleType>
									<xsd:restriction base="TextType">
										<xsd:enumeration value="FORM 4684"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Other Income Description And Amount -->
			<xsd:element name="OtherIncomeDescAndAmt" type="USItemizedEntryType" minOccurs="0" maxOccurs="100">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other Income Description And Amount</Description>
						<LineNumber>8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Other Income Amount -->
			<xsd:element name="TotalOtherIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Other Income Amount</Description>
						<LineNumber>8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Income Amount -->
			<xsd:element name="TotalIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Income Amount</Description>
						<LineNumber>9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Form 4952 Indicator -->
			<xsd:element name="Form4952Ind" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Form 4952 Indicator</Description>
						<LineNumber>10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS4952"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Interest Amount -->
			<xsd:element name="InterestAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Interest Amount</Description>
						<LineNumber>10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxes Amount -->
			<xsd:element name="TaxesAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxes Amount</Description>
						<LineNumber>11</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fiduciary Fees Amount -->
			<xsd:element name="FiduciaryFeesAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fiduciary Fees Amount</Description>
						<LineNumber>12</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Charitable Deductions Amount -->
			<xsd:element name="CharitableDeductionsAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Charitable Deductions Amount</Description>
						<LineNumber>13</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tax Preparation Fees Amount -->
			<xsd:element name="TaxPreparationFeesAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax Preparation Fees Amount</Description>
						<LineNumber>14</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Deductions Amount -->
			<xsd:element name="OtherDeductionsAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other Deductions Amount</Description>
						<LineNumber>15a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS4684 DeductionsOtherCategoriesSchedule"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Net Operating Loss Deduction Amount -->
			<xsd:element name="NetOperatingLossDeductionAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net Operating Loss Deduction Amount</Description>
						<LineNumber>15b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="NOLDeductionStatement"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Miscellaneous Itemized Deductions Amount -->
			<xsd:element name="MiscItemizedDeductionsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Miscellaneous Itemized Deductions Amount</Description>
						<LineNumber>15c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

