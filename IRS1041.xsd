			<xsd:element name="AccountingIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Accounting Income Amount</Description>
						<LineNumber>Schedule B Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
