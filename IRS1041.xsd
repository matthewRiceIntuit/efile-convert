				</xsd:annotation>
			</xsd:element>

			<!-- Capital Gain Allocated to Corpus Amount -->
			<xsd:element name="CapGainAllocatedCorpusAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Capital Gain Allocated to Corpus Amount</Description>
						<LineNumber>Schedule B Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Capital Distributions Amount -->
			<xsd:element name="TotalCapitalDistributionsAmt" type="USAmountType" minOccurs="0">
