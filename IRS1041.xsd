			<!-- First Tier Distribution Amount (Income required to be distributed currently) -->
			<xsd:element name="FirstTierDistributionAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Income required to be distributed currently</Description>
						<LineNumber>Schedule B Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

