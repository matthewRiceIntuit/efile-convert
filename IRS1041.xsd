							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1041ScheduleI"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Total Tax For Tax Year Amount -->
			<xsd:element name="TotalTaxForTaxYearAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Tax For Tax Year Amount</Description>
						<LineNumber>Schedule G Line 1d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Foreign Tax Credit Amount -->
			<xsd:element name="ForeignTaxCreditAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign Tax Credit Amount</Description>
						<LineNumber>Schedule G Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1116"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Current Year General Business Credit Allowed Amount -->
			<xsd:element name="CYGenBusinessCreditAllowedAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current Year General Business Credit Allowed Amount</Description>
						<LineNumber>Schedule G Line 2b</LineNumber>
					</xsd:documentation>
