			<!-- Estate Trust Net Investment Income Tax Amount -->
			<xsd:element name="EstTrustNetInvstIncomeTaxAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net Investment Income Tax from Form 8960</Description>
						<LineNumber>Schedule G Line 4</LineNumber>
						<ELFFieldNumber/>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS8960"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Form 4255 Indicator -->
			<xsd:element name="Form4255Ind" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Form 4255 Indicator</Description>
						<LineNumber>Schedule G Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS4255"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Form 8611 Indicator -->
			<xsd:element name="Form8611Ind" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Form 8611 Indicator</Description>
						<LineNumber>Schedule G Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS8611"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Recapture Tax Code -->
			<xsd:element name="RecaptureTaxCd" minOccurs="0" maxOccurs="2">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Recapture Tax Code</Description>
						<LineNumber>Schedule G Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:enumeration value="ICR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>ICR (investment credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="LIHCR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>LIHCR (low income housing credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="QEVCR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>QEVCR (qualified electric vehicle credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="IECR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>IECR (indian employment credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="NMCR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>NMCR (new markets credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="ECCFR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>ECCFR (employer provided child care faciltities credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="AMVCR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>AMVCR (alternative motor vehicle credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="ARPCR">
							<xsd:annotation>
								<xsd:documentation>
									<Description>ARPCR (alternative fuel vehicle refueling credit)</Description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Recapture Tax Amount -->
			<xsd:element name="RecaptureTaxAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Recapture Tax Amount</Description>
						<LineNumber>Schedule G Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Household Employment Tax Amount -->
			<xsd:element name="HouseholdEmploymentTaxAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Household Employment Tax Amount</Description>
						<LineNumber>Schedule G Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1040ScheduleH"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="F1041TotalTaxGrp" minOccurs="0" maxOccurs="8">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Additional Tax Codes -->
						<xsd:element name="F1041TotalTaxLiteralCd">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Additional Tax Codes</Description>
									<LineNumber>Schedule G Line 7</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:enumeration value="SECT 641(c)"/>
									<xsd:enumeration value="SECT 453(I) INTEREST"/>
									<xsd:enumeration value="SECT 453A(c) INTEREST"/>
									<xsd:enumeration value="FROM FORM 4970"/>
									<xsd:enumeration value="FROM FORM 8697"/>
									<xsd:enumeration value="FROM FORM 8866"/>
									<xsd:enumeration value="SECT 1260(b)"/>
									<xsd:enumeration value="FROM FORM 5329"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<!-- Tax Due Amount (Enter the amount for each literal code for Line 7) -->
						<xsd:element name="TaxDueAmt" type="USAmountType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Enter the amount for each literal code for Line 7</Description>
									<LineNumber>Schedule G Line 7</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Total Tax Amount (Enter the Total Tax including the amounts identified separately with the literal codes) -->
			<xsd:element name="TotalTaxAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the Total Tax including the amounts identified separately with the literal codes</Description>
						<LineNumber>Schedule G Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="ElectingSmallBusinessTrustTaxComputationStatement InterestOnDeferredTaxStatement IRS4970"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Other Information -->

			<!-- Tax Exempt Income Indicator -->
			<xsd:element name="TaxExemptIncomeInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax Exempt Income Indicator</Description>
						<LineNumber>Other Information Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="TaxExemptExpenseAllocationStatement"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Tax Exempt Interest Amount -->
			<xsd:element name="TaxExemptInterestAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax Exempt Interest Amount</Description>
						<LineNumber>Other Information Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Individual Earnings Indicator -->
			<xsd:element name="IndividualEarningsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Individual Earnings Indicator</Description>
						<LineNumber>Other Information Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Foreign Accounts Question Indicator -->
			<xsd:element name="ForeignAccountsQuestionInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign Accounts Question Indicator</Description>
						<LineNumber>Other Information Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Foreign Country Code -->
			<xsd:element name="ForeignCountryCd" type="CountryType" minOccurs="0" maxOccurs="50">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign Country Code</Description>
						<LineNumber>Other Information Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Foreign Trust Indicator -->
			<xsd:element name="ForeignTrustInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign Trust Indicator</Description>
						<LineNumber>Other Information Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Seller Financed Mortgage Indicator -->
			<xsd:element name="SellerFinancedMortgageInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Seller Financed Mortgage Indicator</Description>
						<LineNumber>Other Information Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="InterestPaidOnSellerProvidedFinancingStatement InterestReceivedOrAccruedStatement"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Estate Or Trust Section 663(b) Election Indicator -->
			<xsd:element name="EstateOrTrust663bElectionInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Estate Or Trust Section 663(b) Election Indicator</Description>
						<LineNumber>Other Information Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Section 643(e)(3) Indicator -->
			<xsd:element name="Section643e3Ind" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section 643(e)(3) Indicator</Description>
						<LineNumber>Other Information Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1041ScheduleD"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Decedent Estate 2 Years Indicator -->
			<xsd:element name="DecedentEstate2YearsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Decedent Estate 2 Years Indicator</Description>
						<LineNumber>Other Information Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Trust Beneficiaries Skip Indicator -->
			<xsd:element name="TrustBeneficiariesSkipInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Trust Beneficiaries Skip Indicator</Description>
						<LineNumber>Other Information Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<!-- A 7-digit Non-negative Integer Type -->
	<xsd:simpleType name="IntegerNN7Type">
		<xsd:annotation>
			<xsd:documentation>7-digit non-negative integer type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="IntegerNNType">
			<xsd:totalDigits value="7"/>
		</xsd:restriction>
	</xsd:simpleType>

</xsd:schema>
