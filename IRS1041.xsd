						<xsd:element name="EstimatedTaxCreditedToTrustCd">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Estimated Tax Credited To Trust Code</Description>
									<LineNumber>24a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:enumeration value="SECT 643(G)"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<!-- Estimated Tax Credited To Trust Amount -->
						<xsd:element name="EstimatedTaxCreditedToTrustAmt" type="USAmountType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Estimated Tax Credited To Trust Amount</Description>
									<LineNumber>24a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
