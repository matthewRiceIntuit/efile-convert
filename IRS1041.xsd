			<xsd:element name="CYBondCreditAllowedAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current Year Bond Credit Allowed Amount</Description>
						<LineNumber>Schedule G Line 2d</LineNumber>
