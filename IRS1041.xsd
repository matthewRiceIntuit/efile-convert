			<xsd:element name="CYNonrefundableAMTCrAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current Year Nonrefundable AMT Credits Amount</Description>
						<LineNumber>Schedule G Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS8801"/>
